# Telnyx API Configuration
# Obtén tu API Key desde: https://portal.telnyx.com/#/app/api-keys
TELNYX_API_KEY=your_telnyx_api_key_here

# Telnyx Public Key (para cliente)
TELNYX_PUBLIC_KEY=your_telnyx_public_key_here

# URL de la aplicación
NEXT_PUBLIC_APP_URL=http://localhost:3000

# URL para webhooks de Telnyx (usar ngrok o similar en desarrollo)
NEXT_PUBLIC_TELNYX_WEBHOOK_URL=https://your-domain.com/api/webhooks/telnyx
